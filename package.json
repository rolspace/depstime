  "license": "MIT",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "build": "babel src/ -d dist/",
    "eslint": "eslint .",
    "prepublishOnly": "npm test && npm run build",
    "test": "nyc --reporter=text mocha --require @babel/register ./tests/**"
  },
  "bin": {
    "depstime": "bin/depstime"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rolspace/depstime"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
