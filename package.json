{
  "name": "depstime",
  "version": "1.1.0",
  "author": "Rolando Ramos",
  "description": "Displays the time differences between the locally installed version of a package, the wanted version on your package.json configuration, and the latest available version.",
  "license": "MIT",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=18.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rolspace/depstime.git"
  },
  "scripts": {
    "build": "babel src/ -d dist/",
    "format": "prettier -w .",
    "lint": "eslint .",
    "prepublishOnly": "npm test && npm run build",
    "test": "nyc --reporter=text mocha --require @babel/register ./tests/**"
  },
  "bin": {
    "depstime": "bin/depstime"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "humanize-duration": "^3.27.1",
    "jsonfile": "^6.1.0",
    "moment": "^2.29.1",
    "semver": "^7.3.5",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/register": "^7.16.5",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.5.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "install": "^0.13.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier-config-standard": "^7.0.0",
    "sinon": "^12.0.1"
  }
}
