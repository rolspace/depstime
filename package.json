{
  "name": "depstime",
  "version": "1.1.0",
  "author": "Rolando Ramos",
  "description": "Displays the time differences between the locally installed version of a package, the wanted version on your package.json configuration, and the latest available version.",
  "license": "MIT",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=18.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rolspace/depstime.git"
  },
  "scripts": {
    "build": "babel src/ -d dist/",
    "format": "prettier -w .",
    "lint": "eslint .",
    "prepublishOnly": "npm test && npm run build",
    "test": "mocha --require @babel/register ./tests/**",
    "testWithCoverage": "nyc --reporter lcov --reporter=text mocha --require @babel/register ./tests/**"
  },
  "bin": {
    "depstime": "bin/depstime"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "humanize-duration": "^3.27.1",
    "jsonfile": "^6.1.0",
    "moment": "^2.29.1",
    "semver": "^7.3.5",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/register": "^7.23.7",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "^5.1.1",
    "chai-as-promised": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "install": "^0.13.0",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "prettier-config-standard": "^7.0.0",
    "sinon": "^18.0.0"
  }
}
